// 2) Map each zone ‚Üí its floor label
const zoneFloorMap = {
  // Pune podium
  "Red Zone":                    "Podium Floor",
  "Yellow Zone":                 "Podium Floor",
  "Reception Area":              "Podium Floor",
  "Green Zone":                  "Podium Floor",
  "Orange Zone":                 "Podium Floor",
  "Gsoc Zone":                   "Podium Floor",
  "Assembly Area":               "Podium Floor",

  // Pune 2nd floor
  "2nd Floor, Pune":             "2nd Floor",

  // Pune Tower B
  "Tower B":                     "Tower B",
  "Tower B GYM":                 "Tower B",

  // Outer-Area variants
  "Red Zone - Outer Area":       "Podium Floor",
  "Yellow Zone - Outer Area":    "Podium Floor",
  "Reception Area - Outer Area": "Podium Floor",
  "Green Zone - Outer Area":     "Podium Floor",
  "Orange Zone - Outer Area":    "Podium Floor",
  "2nd Floor, Pune - Outer Area":"2nd Floor",
  "Tower B - Outer Area":        "Tower B",
  "Tower B GYM - Outer Area":    "Tower B",

  // Evicted (‚ÄúOut of office‚Äù) stays on source floor:
   "Out of office":               "Podium Floor",

  // KL, Manila, Tokyo, Taguig
  "Kuala Lumpur":                "Kuala Lumpur",
  "6th Floor":                   "6th Floor",
  "7th Floor":                   "7th Floor",
  "Tokyo":                       "Tokyo",
  "Taguig":                      "Taguig",
};



// 3) How to derive partition for PartitionName2
const partitionMap = {
  'APAC_IN_PUN':    'Pune',
  'APAC_PH_Manila': 'Quezon City',
  'APAC_JPN_Tokyo': 'JP.Tokyo',
  'APAC_MY_KL':     'MY.Kuala Lumpur',
  'APAC_PI_Manila': 'Taguig City'
};


// 4) helper to normalize any door string
function normalizeDoorName(name) {
  return name
    .toUpperCase()                               // case‚Äêinsensitive
    .replace(/_(?:[0-9A-F]{2}:?)+$/i, '')        // strip trailing hex IDs like _10:05:4B
    .replace(/EMTRY\b/, 'ENTRY')                 // fix typo ‚ÄúEMTRY ‚Üí ENTRY‚Äù
    .replace(/POD\s*PODIUM/i, 'PODIUM')          // normalize ‚ÄúPOD podium‚Äù ‚Üí ‚ÄúPODIUM‚Äù
    .replace(/RESTRICTED DOOR/i, 'RESTRICTED DOOR')
    .replace(/\s+/g, ' ')                        // collapse whitespace
    .trim();
}

// 5) Build normalized map entries
const doorMap = Object.entries(doorZoneMap).reduce((acc, [key, zone]) => {
  const [doorName, dir] = key.split('___');

  const prefix    = doorName.split('_').slice(0,3).join('_');
  const partition = partitionMap[prefix] || prefix;
  const floor     = zoneFloorMap[zone]  || 'Unknown';

  if (!acc[doorName]) {
    acc[doorName] = {
      partition,
      door: doorName,
      inDirectionFloor:  null,
      outDirectionFloor: null,
      normalizedDoor:    normalizeDoorName(doorName)
    };
  }
  if (dir === 'InDirection')  acc[doorName].inDirectionFloor  = floor;
  else                         acc[doorName].outDirectionFloor = floor;

  return acc;
}, {});


// Also export the raw door‚Üízone lookup:
module.exports = {
  doorMap: Object.values(doorMap),
  doorZoneMap
};





